@section Styles {
    <link href="@Url.Content("~/Content/calendar.css")" rel="stylesheet" type="text/css" />
}
@{
    ViewBag.Title = "Upcoming Events";
    var dateTime = new DateTime(Convert.ToInt32(ViewBag.RequestedYear), Convert.ToInt32(ViewBag.RequestedMonth), 1, 1, 1, 1);
    var previousMonth = dateTime.AddMonths(-1);
    var nextMonth = dateTime.AddMonths(1);
    var one = dateTime;
    var two = dateTime.AddDays(1);
    var three = dateTime.AddDays(2);
    var four = dateTime.AddDays(3);
    var five = dateTime.AddDays(4);
    var six = dateTime.AddDays(5);
    var seven = dateTime.AddDays(6);
    var eight = dateTime.AddDays(7);
    var nine = dateTime.AddDays(8);
    var ten = dateTime.AddDays(9);
    var eleven = dateTime.AddDays(10);
    var twelve = dateTime.AddDays(11);
    var thirteen = dateTime.AddDays(12);
    var fourteen = dateTime.AddDays(13);
    var fifteen = dateTime.AddDays(14);
    var sixteen = dateTime.AddDays(15);
    var seventeen = dateTime.AddDays(16);
    var eightteen = dateTime.AddDays(17);
    var nineteen = dateTime.AddDays(18);
    var twenty = dateTime.AddDays(19);
    var twentyone = dateTime.AddDays(20);
    var twentytwo = dateTime.AddDays(21);
    var twentythree = dateTime.AddDays(22);
    var twentyfour = dateTime.AddDays(23);
    var twentyfive = dateTime.AddDays(24);
    var twentysix = dateTime.AddDays(25);
    var twentyseven = dateTime.AddDays(26);
    var twentyeight = dateTime.AddDays(27);
    var twentynine = dateTime.AddDays(28);
    var thirty = dateTime.AddDays(29);
    var thirtyone = dateTime.AddDays(30);

    Sport_E.Models.ApplicationDbContext db = new Sport_E.Models.ApplicationDbContext();
    var events = db.Events.Where(e => e.EventDate.Month.Equals(dateTime.Month));
    events = events.Where(e => e.EventDate.Year.Equals(dateTime.Year));

}

